{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product_name": {
      "type": "string"
    },
    "price": {
      "type": "float"
    },
    "product_image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "dimension": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "weight": {
      "type": "string"
    },
    "manufacture_date": {
      "type": "date"
    },
    "expiration_date": {
      "type": "datetime"
    },
    "product_description": {
      "type": "text"
    },
    "is_deleted": {
      "type": "boolean",
      "default": false
    }
  }
}
